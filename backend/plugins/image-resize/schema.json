{
  "upload": {
    "multiple": false,
    "maxFiles": 1,
    "types": ["image/jpeg", "image/png", "image/webp"],
    "maxSize": 52428800
  },
  "options": [
    {
      "name": "width",
      "type": "number",
      "label": { "en": "Width (px)", "zh": "宽度 (px)", "es": "Ancho (px)" },
      "placeholder": { "en": "e.g. 800", "zh": "例如 800", "es": "ej. 800" }
    },
    {
      "name": "height",
      "type": "number",
      "label": { "en": "Height (px)", "zh": "高度 (px)", "es": "Alto (px)" },
      "placeholder": { "en": "e.g. 600", "zh": "例如 600", "es": "ej. 600" }
    }
  ],
  "submitText": {
    "en": "Resize Image",
    "zh": "调整尺寸",
    "es": "Redimensionar"
  },
  "outputType": "file"
}
